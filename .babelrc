{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "useBuiltIns": "entry",
                "corejs": 3,
                "targets": {
                    "esmodules": true,
                    "browsers": "> 1%, not ie 11, not op_mini all",
                    "node": 10
                }
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/transform-runtime",
        [
            "import",
            {
                "libraryName": "@material-ui/icons",
                "libraryDirectory": "",
                "camel2DashComponentName": false
            }
        ]
    ]
}